[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[submodule]
	active = .
[remote "origin"]
	url = https://github.com/SeanMcFarlane/BCIJam_2021.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "main"]
	remote = origin
	merge = refs/heads/main

[merge]
	tool = unityyamlmerge

[mergetool "unityyamlmerge"]
	cmd = 'C:/Program Files/Unity/Hub/Editor/2021.1.18f1/Editor/Data/Tools/UnityYAMLMerge.exe' merge -p "$BASE" "$REMOTE" "$LOCAL" "$MERGED"
	trustExitCode = false